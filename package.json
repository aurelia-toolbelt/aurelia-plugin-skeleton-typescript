{
    "name": "awsome",
    "version": "1.0.0-beta.1",
    "description": "aurelia-plugin-skeleton-typescript",
    "keywords": [
        "aurelia",
        "plugin",
        "skeleton",
        "typescript"
    ],
    "homepage": "https://github.com/vegarringdal/aurelia-plugin-skeleton-typescript",
    "bugs": {
        "url": "https://github.com/vegarringdal/aurelia-plugin-skeleton-typescript/issues"
    },
    "license": "MIT",
    "author": "Vegar Ringdal (vegar.ringdal@gmail.com)",
    "main": "dist/commonjs/index.js",
    "typings": "dist/commonjs/index.d.ts",
    "ts:main": "distTS/index.ts",
    "types": "dist/commonjs/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/aurelia-plugin-skeleton-typescript/vGrid"
    },
    "scripts": {
        "build": "node ./scripts/build.js",
        "setup": "node ./scripts/setup.js",
        "watch": "node ./scripts/watch.js",
        "eslint": "node ./node_modules/eslint/bin/eslint scripts",
        "eslintFix": "node ./node_modules/eslint/bin/eslint scripts --fix"
    },
    "devDependencies": {
        "aurelia-animator-css": "^1.0.4",
        "aurelia-binding": "^2.0.0",
        "aurelia-bootstrapper": "^2.2.0",
        "aurelia-dependency-injection": "^1.3.2",
        "aurelia-fetch-client": "^1.3.1",
        "aurelia-framework": "^1.2.0",
        "aurelia-loader": "^1.0.0",
        "aurelia-metadata": "^1.0.3",
        "aurelia-pal": "^1.8.0",
        "aurelia-path": "^1.1.1",
        "aurelia-task-queue": "^1.3.0",
        "aurelia-templating": "^1.7.0",
        "eslint": "^5.0.0-alpha.4",
        "fuse-box": "^3.3.0-next.9",
        "fuse-box-aurelia-loader": "^2.0.2",
        "fuse-box-typechecker": "^2.8.0",
        "less": "^3.0.4",
        "source-map": "^0.7.3",
        "stylus": "^0.54.5",
        "tslint": "^5.10.0",
        "typescript": "2.9.1"
    },
    "jspm": {
        "registry": "npm",
        "main": "index",
        "format": "cjs",
        "directories": {
            "dist": "dist/commonjs"
        },
        "devDependencies": {}
    },
    "dependencies": {
        "@types/node": "^10.3.0"
    },
    "aurelia": {
        "usedBy": [],
        "build": {
            "resources": [
                "awsome",
                "awsome/index",
                "awsome/hello-world.css",
                "awsome/hello-world.html"
            ]
        }
    }
}